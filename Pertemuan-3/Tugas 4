a = int(input("a:"))
b = int(input("b:"))
c = int(input("c:"))

if (a == b) and (b == c) and ( a == c ):
    print("Equilateral")

elif ( a == b ) or ( b == c ) or ( a == c ):
    print("Isosceles")

elif ( a >= b + c ) or ( b >= c + a ) or ( c >= a + b ):
    print("Not a Triangle")

elif ( a ** 2 ) or ( b ** 2 ) or ( c ** 2 ):
    print("Scalene")

else:
    print("Right-angled")
